
public class QProgram {

	public static void main(String[] args) {
		double deno=365;
		double ans=1;
		System.out.print("1-");
		for(double i=364;i>350;i--){
			ans=ans*(i/deno);
			System.out.print("("+i+"/"+deno+")*");
		}
		System.out.println("\n"+ans);
	}
	
	
	
